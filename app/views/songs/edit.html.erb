<%= form_for @song do |f| %>
    <%=f.text_field :title %>
    <%=f.check_box :released  %>
    <%=f.number_field :release_year %>
    <%=f.text_field :artist_name %>
    <%=f.text_field :genre %>
    <%=f.submit :Edit %>
<% end %>

<%= form_for @song, url: "#{song_path @song}", method: 'delete' do |f|%>
    <%= f.submit :delete %>
<% end %>
            